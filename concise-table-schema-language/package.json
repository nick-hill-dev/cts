{
  "name": "concise-table-schema-language",
  "displayName": "Concise Table Schema Language",
  "description": "Enables syntax highlighting for CTS files.",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "cts",
        "aliases": [
          "Concise Table Schema",
          "cts"
        ],
        "extensions": [
          ".cts"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "cts",
        "scopeName": "source.cts",
        "path": "./syntaxes/cts.tmLanguage.json",
        "embeddedLanguages": {
          "meta.embedded.block.json": "json"
        }
      }
    ],
    "snippets": [
      {
        "language": "cts",
        "path": "./snippets/cts.code-snippets"
      }
    ]
  }
}